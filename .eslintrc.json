{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "classes": true,
      "jsx": true
    }
  },
  "rules": {
    "indent": ["error", 2, { "SwitchCase": 1 }],
    "linebreak-style": ["error", "unix"],
    "jsx-quotes": [1, "prefer-double"],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "react/no-find-dom-node": "warn",
    "react/jsx-no-bind": [
      "warn",
      {
        "ignoreRefs": true,
        "allowArrowFunctions": false,
        "allowFunctions": false,
        "allowBind": false
      }
    ],
    "no-console": ["warn", { "allow": ["warn", "error", "info", "group", "groupEnd"] }],
    "comma-dangle": 1,
    "no-cond-assign": 1,
    "no-constant-condition": 0,
    "no-control-regex": 0,
    "no-debugger": 1,
    "no-dupe-keys": 1,
    "no-empty": 1,
    "no-empty-character-class": 1,
    "no-ex-assign": 0,
    "no-extra-boolean-cast": 0,
    "no-extra-parens": 0,
    "no-extra-semi": 1,
    "no-func-assign": 0,
    "no-inner-declarations": 0,
    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 0,
    "no-obj-calls": 0,
    "no-regex-spaces": 0,
    "quote-props": 0,
    "no-sparse-arrays": 0,
    "no-unreachable": 0,
    "use-isnan": 0,
    "valid-jsdoc": 0,
    "valid-typeof": 0,
    "block-scoped-var": 1,
    "complexity": 0,
    "consistent-return": 0,
    "curly": 1,
    "default-case": 1,
    "dot-notation": 0,
    "eqeqeq": 1,
    "guard-for-in": 0,
    "no-alert": 0,
    "no-caller": 1,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-labels": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 0,
    "no-extra-bind": 0,
    "no-fallthrough": 0,
    "no-floating-decimal": 0,
    "no-implied-eval": 0,
    "no-iterator": 0,
    "no-lone-blocks": 0,
    "no-loop-func": 1,
    "no-multi-spaces": 1,
    "no-multi-str": 1,
    "no-native-reassign": 0,
    "no-new": 0,
    "no-new-func": 0,
    "no-new-wrappers": 0,
    "no-octal": 0,
    "no-octal-escape": 0,
    "no-process-env": 0,
    "no-proto": 0,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-script-url": 0,
    "no-self-compare": 1,
    "no-sequences": 0,
    "no-unused-expressions": 0,
    "no-void": 0,
    "no-warning-comments": 0,
    "no-with": 1,
    "radix": 0,
    "vars-on-top": 0,
    "wrap-iife": 0,
    "yoda": 1,
    "global-strict": 0,
    "no-extra-strict": 0,
    "strict": 0,
    "no-catch-shadow": 0,
    "no-delete-var": 0,
    "no-label-var": 0,
    "no-shadow": 0,
    "no-shadow-restricted-names": 0,
    "no-undef": 0,
    "no-undef-init": 0,
    "no-undefined": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "brace-style": 1,
    "camelcase": 0,
    "comma-spacing": 0,
    "comma-style": 1,
    "consistent-this": 0,
    "eol-last": 1,
    "func-names": 0,
    "func-style": 0,
    "key-spacing": 0,
    "max-nested-callbacks": 0,
    "new-cap": 0,
    "new-parens": 0,
    "no-array-constructor": 0,
    "no-inline-comments": 0,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": 1,
    "no-nested-ternary": 1,
    "no-new-object": 0,
    "semi-spacing": 1,
    "no-spaced-func": 1,
    "no-ternary": 0,
    "no-trailing-spaces": 1,
    "no-underscore-dangle": 0,
    "no-wrap-func": 0,
    "one-var": 0,
    "operator-assignment": 0,
    "padded-blocks": 0,
    "quotes": [
      2,
      "single",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ],
    "semi": 0,
    "sort-vars": 0,
    "space-after-function-name": 0,
    "space-after-keywords": 0,
    "space-before-blocks": 2,
    "object-curly-spacing": 0,
    "space-in-parens": 0,
    "space-infix-ops": 1,
    "space-return-throw-case": 0,
    "space-unary-ops": 0,
    "spaced-comment": 1,
    "wrap-regex": 0,
    "no-var": 0,
    "generator-star": 0,
    "max-depth": 0,
    "max-len": [
      1,
      120,
      4,
      {
        "ignoreUrls": true
      }
    ],
    "max-params": 0,
    "max-statements": 0,
    "no-bitwise": 0,
    "no-plusplus": 0
  }
}
